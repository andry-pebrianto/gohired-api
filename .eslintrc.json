{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": 0,
        "no-lonely-if": 0,
        "no-param-reassign": 0,
        "default-param-last": 0,
        "brace-style": 0,
        "consistent-return": 0,
        "prefer-destructuring": 0,
        "max-len": 0,
        "camelcase": 0,
        "no-await-in-loop": 0,
        "no-plusplus": 0
    }
}
